# LinkedIn Credentials (DO NOT COMMIT REAL CREDENTIALS)
LINKEDIN_EMAIL=your-email@example.com
LINKEDIN_PASSWORD=your-password-here

# Database
DATABASE_PATH=./data/linkedin.db

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/automation.log

# Browser Settings
HEADLESS=false
BROWSER_TIMEOUT=30

# Rate Limiting
MAX_CONNECTIONS_PER_DAY=20
MAX_MESSAGES_PER_DAY=10
MIN_DELAY_SECONDS=5
MAX_DELAY_SECONDS=15

# Search Settings
SEARCH_KEYWORDS=Software Engineer,Product Manager
MAX_SEARCH_PAGES=3

# Messaging
MESSAGE_TEMPLATE=Hi {name}, I noticed your profile and would love to connect!

# Stealth Settings
ENABLE_STEALTH=true
RANDOMIZE_VIEWPORT=true
CUSTOM_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36
